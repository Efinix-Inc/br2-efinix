CONFIG_SECTION_MISMATCH_WARN_ONLY=n

CONFIG_BLK_DEV_INITRD=y
CONFIG_INITRAMFS_SOURCE=""
CONFIG_RD_GZIP=y
CONFIG_RD_BZIP2=y
CONFIG_RD_LZMA=y
CONFIG_RD_XZ=y
CONFIG_RD_LZO=y
CONFIG_RD_LZ4=y

CONFIG_PRINTK_TIME=y
CONFIG_TMPFS=y

# Filesystem
CONFIG_MMC=y
CONFIG_MMC_SPI=y
CONFIG_EXT2_FS=y
CONFIG_EXT4_FS=y
CONFIG_VFAT_FS=y

# Native Support Language
CONFIG_NLS=y
CONFIG_NLS_DEFAULT="utf8"
CONFIG_NLS_CODEPAGE_437=y
CONFIG_NLS_ISO8859_1=m
CONFIG_NLS_ASCII=m
CONFIG_NLS_UTF8=m

# LEDS
CONFIG_NEW_LEDS=y
CONFIG_LEDS_CLASS=y
CONFIG_LEDS_GPIO=y
CONFIG_LEDS_PWM=y
CONFIG_LEDS_TRIGGERS=y
CONFIG_LEDS_TRIGGER_HEARTBEAT=y
CONFIG_LEDS_TRIGGER_GPIO=y
CONFIG_LEDS_TRIGGER_TIMER=y
CONFIG_LEDS_TRIGGER_ONESHOT=y
CONFIG_LEDS_TRIGGER_BACKLIGHT=y
CONFIG_LEDS_TRIGGER_CPU=y
CONFIG_LEDS_TRIGGER_DEFAULT_ON=y

# NET
CONFIG_NET=y
CONFIG_PACKET=y
CONFIG_UNIX=y
CONFIG_INET=y
CONFIG_NETDEVICES=y
CONFIG_PACKET_DIAG=y
CONFIG_NET_PACKET_ENGINE=y
CONFIG_NET_CORE=y
CONFIG_ETHERNET=y
CONFIG_VIRTIO_NET=y
CONFIG_PHYLINK=y
CONFIG_PHYLIB=y
CONFIG_SWPHY=y
CONFIG_FIXED_PHY=y

CONFIG_EFI=n
CONFIG_STRICT_KERNEL_RWX=n

# UART
CONFIG_SERIAL_SPINAL_LIB_UART=y
CONFIG_SERIAL_SPINAL_LIB_UART_BAUDRATE=115200
CONFIG_SERIAL_SPINAL_LIB_UART_CONSOLE=y

CONFIG_IKCONFIG=y
CONFIG_IKCONFIG_PROC=y

CONFIG_HOTPLUG_CPU=y
CONFIG_MODULES=y
CONFIG_MODULE_UNLOAD=y
CONFIG_MODULE_FORCE_UNLOAD=y
CONFIG_MODVERSIONS=y

CONFIG_CGROUPS=y
