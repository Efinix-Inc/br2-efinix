CONFIG_SECTION_MISMATCH_WARN_ONLY=n

# Architecture
CONFIG_ARCH_DEFCONFIG="arch/riscv/configs/rv32_defconfig"
CONFIG_ARCH_RV32I=y
CONFIG_RISCV_ISA_M=y
CONFIG_RISCV_ISA_A=y
CONFIG_RISCV_ISA_C=n
CONFIG_SIFIVE_PLIC=y
CONFIG_FPU=y
CONFIG_SMP=y

CONFIG_BLK_DEV_INITRD=y
CONFIG_INITRAMFS_SOURCE=""
CONFIG_RD_GZIP=y
CONFIG_RD_BZIP2=y
CONFIG_RD_LZMA=y
CONFIG_RD_XZ=y
CONFIG_RD_LZO=y
CONFIG_RD_LZ4=y

CONFIG_SERIAL_EARLYCON_RISCV_SBI=y
CONFIG_PRINTK_TIME=y
CONFIG_TMPFS=y


# SPI
CONFIG_SPI=y
CONFIG_SPI_MASTER=y
CONFIG_EXPERIMENTAL=y
CONFIG_SPI_SPIDEV=y
CONFIG_SPI_SPINAL_LIB=y
CONFIG_SPI_ALTERA=y
CONFIG_SND_SOC_TLV320AIC26=y
CONFIG_SPI_LOOPBACK_TEST=y
CONFIG_MTD=y
CONFIG_MTD_SPI_NOR=y
CONFIG_MTD_M25P80=y


# Filesystem
CONFIG_MMC=y
CONFIG_MMC_SPI=y
CONFIG_EXT2_FS=y

# GPIO
CONFIG_GPIOLIB=y
CONFIG_GPIO_SYSFS=y
CONFIG_OF_GPIO=y
CONFIG_GPIO_SPINAL_LIB=y

# LEDS
CONFIG_NEW_LEDS=y
CONFIG_LEDS_CLASS=y
CONFIG_LEDS_GPIO=y
CONFIG_LEDS_PWM=y
CONFIG_LEDS_TRIGGERS=y
CONFIG_LEDS_TRIGGER_HEARTBEAT=y
CONFIG_LEDS_TRIGGER_GPIO=y
CONFIG_LEDS_TRIGGER_TIMER=y
CONFIG_LEDS_TRIGGER_ONESHOT=y
CONFIG_LEDS_TRIGGER_BACKLIGHT=y
CONFIG_LEDS_TRIGGER_CPU=y
CONFIG_LEDS_TRIGGER_DEFAULT_ON=y

# NET
CONFIG_NET=n
CONFIG_PACKET=n
CONFIG_UNIX=n
CONFIG_INET=n
CONFIG_NETDEVICES=n
CONFIG_PACKET_DIAG=n
CONFIG_NET_PACKET_ENGINE=n

# MAC
CONFIG_NET_VENDOR_SPINAL=n
CONFIG_SPINAL_LIB_MAC=n

# Efinix TSE
CONFIG_NETDEVICES=y
CONFIG_MII=y
CONFIG_NET_CORE=y
CONFIG_ETHERNET=y
CONFIG_EFINIX_TSE=y
CONFIG_PHYLIB=y

# DebugFS
CONFIG_DEBUG_FS=y

CONFIG_EFI=n
CONFIG_STRICT_KERNEL_RWX=n

# UART
CONFIG_SERIAL_SPINAL_LIB_UART=y
CONFIG_SERIAL_SPINAL_LIB_UART_BAUDRATE=115200
CONFIG_SERIAL_SPINAL_LIB_UART_CONSOLE=y
